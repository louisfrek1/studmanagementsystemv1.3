@{
}
@if(TempData["SucessMessage"] != null)
{
    <script>
        alert('@TempData["SucessMessage"]');
        console.info('@TempData["SucessMessage"]');
        
    </script>
}else if(TempData["ErrorMessage"] != null)
{
    <script>
        alert('@TempData["ErrorMessage"]');
        console.error('@TempData["ErrorMessage"]');
    </script>
}

<form method="post">
    <div class="row container mt-5">
        <h1.1>Student's Name: </h1.1>
        <input type="text" class="form-control border-2" name="Name" />
    </div>
    <br>
    <div class="row">
        <br>
        <button id="submitBtn" type="text" class="btn btn-primary">Submit</button>
    </div>
    <script src="site.js">
         </script>
</form>